{
    "name": "linkorb/hatter",
    "type": "library",
    "description": "A database fixture tool inspired by Alice and Haigha, but better suited for non-symfony or third-party application written in other programming languages.",
    "keywords": [
        "hatter",
        "fixtures",
        "mysql-fixtures",
        "mariadb-fixtures",
        "yaml-fixtures"
    ],
    "homepage": "https://github.com/linkorb/hatter",
    "license": "proprietary",
    "authors": [
        {
            "name": "LinkORB Engineering",
            "email": "engineering@linkorb.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "fakerphp/faker": "^1.23",
        "linkorb/connector": "^1.5",
        "linkorb/xuid": "^1.4",
        "symfony/console": "^6.4|^7.2",
        "symfony/dotenv": "^6.4|^7.2",
        "symfony/expression-language": "^6.4|^7.2",
        "symfony/uid": "^6.4|^7.2",
        "symfony/yaml": "^6.4|^7.2"
    },
    "conflict": {
        "symfony/console": "<6.4"
    },
    "autoload": {
        "psr-4": {
            "LinkORB\\Component\\Hatter\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "bin": [ "Resources/bin/hatter" ],
    "archive": {
        "exclude": [
            "/example",
            "/.gitignore",
            "/composer.lock"
        ]
    }
}
